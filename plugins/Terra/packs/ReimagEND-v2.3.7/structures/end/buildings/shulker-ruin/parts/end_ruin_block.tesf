num random = randomInt(3);

str setBlock = "";
if (random == 0) {
    setBlock = "minecraft:end_stone_bricks";
} else if (random == 1) {
    setBlock = "minecraft:purpur_block";
} else if (random == 2) {
    setBlock = "minecraft:end_stone_brick_slab";
} else if (random == 3) {
    setBlock = "minecraft:purpur_slab";
}

block(0,0,0,setBlock);



