num height = 10+((1+randomInt(3))*5);
num radius = 2;
num sideLength = 10;

for (num h = 0; h < height; h = h + 1) {
    num r = radius;
    if (h == height-1) r = 4;
    for (num x = -r; x < r; x = x + 1) {
        for (num y = -r; y < r; y = y + 1) {
            for (num z = -r; z < r; z = z + 1) {
                if (pow(x,2)+pow(y,2)+pow(z,2) < pow(r,2) && h < height-3) {
                    structure(x, h + y, z, "aprichorus_tree_spikes", "NONE");      
                }
            }
        }
    }
    structure(0, height-1, 0, "aprichorus_tree_bulb", "NONE"); 
}

